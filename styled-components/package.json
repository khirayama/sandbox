    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "autoprefixer": "9.4.5",
    "babel-loader": "^8.0.5",
    "concurrently": "4.1.0",
    "cpx": "1.5.0",
    "cssnano": "4.1.8",
    "jest": "23.6.0",
    "postcss-cli": "4.1.1",
    "postcss-import": "12.0.1",
    "postcss-nested": "4.1.1",
    "postcss-simple-vars": "5.0.1",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "styled-components": "4.1.3",
